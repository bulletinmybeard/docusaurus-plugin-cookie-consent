{
  "name": "docusaurus-plugin-cookie-consent",
  "version": "1.0.1",
  "description": "GDPR-compliant cookie consent plugin for Docusaurus with built-in i18n support",
  "main": "dist/plugin/index.js",
  "types": "dist/plugin/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/plugin/index.d.ts",
      "default": "./dist/plugin/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./dist/components/CookieConsent": {
      "types": "./dist/components/CookieConsent/index.d.ts",
      "default": "./dist/components/CookieConsent/index.js"
    },
    "./dist/components/CookieSettingsFooter": {
      "types": "./dist/components/CookieSettingsFooter/index.d.ts",
      "default": "./dist/components/CookieSettingsFooter/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "default": "./dist/hooks/index.js"
    },
    "./dist/hooks/usePluginConfig": {
      "types": "./dist/hooks/usePluginConfig.d.ts",
      "default": "./dist/hooks/usePluginConfig.js"
    },
    "./styles": "./dist/styles/cookie-consent.css",
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./client/hooks": {
      "types": "./dist/client/hooks.d.ts",
      "default": "./dist/client/hooks.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile && npm run copy-assets",
    "clean": "rimraf dist",
    "compile": "tsc",
    "copy-assets": "npm run copy-css && npm run copy-templates",
    "copy-css": "copyfiles -u 1 \"src/**/*.css\" dist",
    "copy-templates": "copyfiles \"templates/**/*\" dist",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "docusaurus",
    "plugin",
    "cookie",
    "consent",
    "gdpr",
    "privacy",
    "analytics",
    "i18n",
    "cookie-consent",
    "cookie-banner",
    "compliance"
  ],
  "author": {
    "name": "Robin Schulz",
    "email": "bulletinmybeard@gmail.com",
    "url": "https://rschu.me"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bulletinmybeard/docusaurus-plugin-cookie-consent.git"
  },
  "bugs": {
    "url": "https://github.com/bulletinmybeard/docusaurus-plugin-cookie-consent/issues"
  },
  "homepage": "https://github.com/bulletinmybeard/docusaurus-plugin-cookie-consent#readme",
  "peerDependencies": {
    "@docusaurus/core": "^3.0.0",
    "@docusaurus/types": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@eslint/js": "^9.0.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.2.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "typescript": "~5.5.4"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "handlebars": "^4.7.8",
    "react-markdown": "^9.1.0",
    "remark-gfm": "^4.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
